  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-plugin-rss": "^1.0.9",
    "@tailwindcss/custom-forms": "^0.2.1",
    "alpinejs": "^2.6.0",
    "cross-env": "^7.0.2",
    "html-minifier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-clean": "^1.1.0",
    "postcss-cli": "^7.1.2",
